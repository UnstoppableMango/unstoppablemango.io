{
  "scripts": {
    "postinstall": "dotnet tool restore",
    "start": "dotnet fable watch src/App --run webpack serve",
    "build": "dotnet fable       src/App --run webpack --mode production",
    "clean:fable": "dotnet fable clean --yes",
    "clean:dotnet": "dotnet clean src/App",
    "clean": "npm run clean:fable && npm run clean:dotnet"
  },
  "devDependencies": {
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.0"
  }
}
